@model IEnumerable<TimiTS.Models.ViewModels.UserListViewModel>

@{
    ViewData["Title"] = "Brukerinstillinger";
}

<h2>Brukerinnstillinger</h2>

<table class="table" id="ProjectTable">
    <thead>
        <tr>
            <th>
                Ansatt Id
            </th>
            <th>
                Navn
            </th>
            <th>
                Epost
            </th>
            <th>
                Handling
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.EId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>

                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-primary btn-sm">Rediger</a>
                </td>
            </tr>
        }
    </tbody>
</table>



